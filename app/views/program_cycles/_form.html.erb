<%= simple_form_for(@program_cycle) do |f| %>
<%= f.error_notification %>

<div class="form-inputs">
      <%= f.input :name %>
      <%= f.input :start_date %>
      <%= f.input :end_date %>
      <%= f.input :goal %>
      <%= f.input :description %>
</div>

<div class="form-actions">
  <%= f.button :submit %>
</div>
<% end %>
